package com.cqupt.project.model;

import java.util.Date;

public class Attachment {
	    private String fileId;

	    private Date timeFileUpload;//上传文件日期

	    private String filePath;//文件保存在系统中的路径

	    private String uploadFileName;//上传文件后保存的名称

	    private String uploadRealName;//上传文件的真实名字

		public Attachment(String fileId, Date timeFileUpload, String filePath,
				String uploadFileName, String uploadRealName) {
			this.fileId = fileId;
			this.timeFileUpload = timeFileUpload;
			this.filePath = filePath;
			this.uploadFileName = uploadFileName;
			this.uploadRealName = uploadRealName;
		}
		public Attachment(){}

		public String getFileId() {
	        return fileId;
	    }

	    public void setFileId(String fileId) {
	        this.fileId = fileId;
	    }

	    public Date getTimeFileUpload() {
	        return timeFileUpload;
	    }

	    public void setTimeFileUpload(Date timeFileUpload) {
	        this.timeFileUpload = timeFileUpload;
	    }

	    public String getFilePath() {
	        return filePath;
	    }

	    public void setFilePath(String filePath) {
	        this.filePath = filePath;
	    }

	    public String getUploadFileName() {
	        return uploadFileName;
	    }

	    public void setUploadFileName(String uploadFileName) {
	        this.uploadFileName = uploadFileName;
	    }

	    public String getUploadRealName() {
	        return uploadRealName;
	    }

	    public void setUploadRealName(String uploadRealName) {
	        this.uploadRealName = uploadRealName;
	    }
}
