package com.cqupt.project.model;

import java.util.ArrayList;
import java.util.Date;

public class Contract {
    private Integer contractId;

    private Integer proId;
    private Project Project;

    private Integer bidId;
    private Bid bid;//合同的标书

    private Company companyA;
    private Integer companyAId;//雇主id

    private Company companyB;

	private Integer companyBId;//服务商id

    private String platform;//监管id
    private Supervisor supservisor;//监管

    private Date signDate;//簽署日期

    private Long tradAmount;//项目金额

    private String fileId;
    
    private ArrayList<Attachment> attachments;//合同的文件
    
    public Integer getContractId() {
        return contractId;
    }

    public void setContractId(Integer contractId) {
        this.contractId = contractId;
    }

    public Integer getProId() {
        return proId;
    }

    public void setProId(Integer proId) {
        this.proId = proId;
    }

    public Integer getBidId() {
        return bidId;
    }

    public void setBidId(Integer bidId) {
        this.bidId = bidId;
    }

    public Integer getCompanyAId() {
        return companyAId;
    }

    public void setCompanyAId(Integer companyAId) {
        this.companyAId = companyAId;
    }

    public Integer getCompanyBId() {
        return companyBId;
    }

    public void setCompanyBId(Integer companyBId) {
        this.companyBId = companyBId;
    }
    public Company getCompanyA() {
		return companyA;
	}

	public void setCompanyA(Company companyA) {
		this.companyA = companyA;
	}

	public Company getCompanyB() {
		return companyB;
	}

	public void setCompanyB(Company companyB) {
		this.companyB = companyB;
	}
    public String getPlatform() {
        return platform;
    }

    public void setPlatform(String platform) {
        this.platform = platform;
    }

    public Date getSignDate() {
        return signDate;
    }

    public void setSignDate(Date signDate) {
        this.signDate = signDate;
    }

    public Long getTradAmount() {
        return tradAmount;
    }

    public void setTradAmount(Long tradAmount) {
        this.tradAmount = tradAmount;
    }

    public String getFileId() {
        return fileId;
    }

    public void setFileId(String fileId) {
        this.fileId = fileId;
    }

	public ArrayList<Attachment> getAttachments() {
		return attachments;
	}

	public void setAttachments(ArrayList<Attachment> attachments) {
		this.attachments = attachments;
	}

	public Project getProject() {
		return Project;
	}

	public void setProject(Project project) {
		Project = project;
	}

	public Bid getBid() {
		return bid;
	}

	public void setBid(Bid bid) {
		this.bid = bid;
	}

	public Supervisor getSupservisor() {
		return supservisor;
	}

	public void setSupservisor(Supervisor supservisor) {
		this.supservisor = supservisor;
	}
}